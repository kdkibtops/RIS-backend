{
  "name": "radassist-backend",
  "version": "1.0.0",
  "description": "Backend for Radiology Assistant",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "create-dev-db": "db-migrate db:create reportingassistdb --env dbmigrateconnect && db-migrate up --env Dev",
    "start": "tsc-watch -p ./tsconfig.json --onSuccess \"node ./dist/server.js\"",
    "startAuth": "tsc-watch -p ./tsconfig.json --onSuccess \"node ./dist/authorizationServer.js\"",
    "build": "npx tsc",
    "run": "node ./dist/server.js",
    "create-production-db": "db-migrate db:create reportingassistdb --env dbmigrateconnect && db-migrate up --env Production",
    "reset-db": "db-migrate db:drop reportingassistdb && db-migrate db:drop reportingassistdb"
  },
  "author": "Mustafa Heidar",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.25",
    "@types/node-fetch": "^2.6.1",
    "@types/pg": "^8.6.5",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "formidable": "^2.1.1",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.0",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.0",
    "pg": "^8.8.0"
  }
}
